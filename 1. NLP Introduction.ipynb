    "import urllib.request as r\n",
    "from bs4 import BeautifulSoup\n",
    "response = r.urlopen(\"http://php.net\")\n",
    "html = response.read()\n",
    "soup = BeautifulSoup(html,\"html5lib\")\n",
    "text = soup.get_text(strip=True)\n",
    "print (text)"
